[clone/clean] ($in) >> ($out)
  children [clean 1] [clean 2]
  nets  
    >> n1 >> [clean 1]
    >> n2 >> [clean 2]
  connections
    [$self] ($in) >> n1 >> [clean 1] ($in)
    [clean 1] ($out) >> n2 >> [clean 2] ($in)
    [clean 2] ($out) >> $nil >> $return

[clone/clean/clean 1] ($in) >> ($out)
  code @{bash make clean}

[top]($in) >> ()
  children [clone] [make] [exec]
  nets
     >> n1 >> [clone]
     >> n2 >> [make]
     >> n3 >> [exec]
  connections
     [$self]($in) >> n1 >> [clone]($in)
     [clone]($out) >> n2 >> [make]($in)
     [make]($out) >> n3 >> [exec]($in)
     [exec]($out) >> $nil >> $return
