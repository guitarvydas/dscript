[top]($in) >> ()
  children [top/clone] [top/make] [top/exec]
  nets
     >> n1 >> [top/clone]
     >> n2 >> [top/make]
     >> n3 >> [top/exec]
  connections
     [$self]($in) >> n1 >> [top/clone]($in)
     [top/clone]($out) >> n2 >> [top/make]($in)
     [top/make]($out) >> n3 >> [top/exec]($in)
     [top/exec]($out) >> $nil >> $return
